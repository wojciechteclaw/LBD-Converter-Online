{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@enums/*": [
                "src/enums/*"
            ],
            "@helpers/*": [
                "src/helpers/*"
            ],
            "@hooks/*": [
